{
  "singleQuote": true,
  "trailingComma": "all",
  "printWidth": 80,
  "endOfLine": "lf",
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^@/(.*)$",
    "",
    "^[./]",
    "",
    "<TYPES>"
  ],
  "importOrderParserPlugins": ["typescript", "jsx", "decorators"],
  "importOrderTypeScriptVersion": "5.4.5",
  "importOrderCaseInsensitive": true
}
